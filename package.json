{
  "name": "@floemia/osu-droid-card",
  "version": "1.2.1",
  "description": "A module that generates a profile card from an osu!droid player.",
  "license": "MIT",
  "author": "floemia",
  "scripts": {
    "build": "tsup"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "assets/**/*"
  ],
  "compilerOptions": {
    "types": [
      "node"
    ]
  },
  "dependencies": {
    "@floemia/osu-droid-utils": "^0.0.7",
    "@napi-rs/canvas": "^0.1.72",
    "@rian8337/osu-base": "4.0.0-beta.74",
    "dotenv": "^16.5.0",
    "fast-average-color-node": "^3.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/node": "^24.0.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/floemia/osu-droid-card.git"
  }
}
