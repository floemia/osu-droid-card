{
  "name": "osu-droid-card",
  "version": "0.0.7-beta",
  "description": "Generates a card of an osu!droid user's profile.",
  "author": {
    "name": "floemia"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/floemia/osu-droid-card.git"
  },
  "keywords": [
    "typescript",
    "osu",
    "web scraping",
	"profile card"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md",
	"examples/**/*",
	"assets/**/*"
  ],
  "scripts": {
    "test": "tsup && node ./dist/test.js",
    "build": "tsup"
  },
  "devDependencies": {
    "@types/node": "^20.2.3",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.62",
    "dotenv": "^16.4.5",
    "fast-average-color-node": "^3.1.0",
    "osu-droid-scraping": "^1.1.5"
  }
}
